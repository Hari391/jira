{
    "key": "addon-key",
    "name": "jira-addon",
    "description": "Addon for exporting filtered task's description based on projects, board and sprints with category",
    "vendor": {
        "name": "abc",
        "url": "www.abc.com"
    },
    "baseUrl": "https://jiraaddon.herokuapp.com",
    "links": {
        "self": "https://jiraaddon.herokuapp.com/atlassian-connect.json",
        "homepage": "https://jiraaddon.herokuapp.com/atlassian-connect.json"
    },
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
          "installed": "/installed",
	  "uninstalled": "/uninstalled"
    },
    "enableLicensing": true,
    "scopes": [
        "READ"
    ],
    "modules": {
        "generalPages": [
            {
                "key": "addon-key",
                "location": "system.top.navigation.bar",
                "name": {
                    "value": "addonabc"
                },
                "url": "/activity",
                "conditions": [{
                    "condition": "user_is_logged_in"
                }]
            } 
        ]
    }
}
